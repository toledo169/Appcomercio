<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.oaxacacomercio">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".password.ConfirmarPasswordActivity"></activity>
        <activity android:name=".password.PasswordActivity" />
        <activity
            android:name=".Eventuales.PermisosEventualActivity"
            android:screenOrientation="locked" />
        <activity android:name=".Mapas.MapaPermiso" />
        <activity android:name=".Mapas.MapaActActivity" />
        <activity
            android:name=".Detalles.DetallesMapaActividaActivity"
            android:screenOrientation="locked" />
        <activity
            android:name=".Detalles.DetallesActividadActivity"
            android:screenOrientation="locked" />
        <activity android:name=".Mapas.MapaorganizacionActivity" />
        <activity
            android:name=".Detalles.DetallesMapaOrganizacionActivity"
            android:screenOrientation="locked" />
        <activity
            android:name=".Detalles.DetallesMapaZonaActivity"
            android:screenOrientation="locked" />
        <activity android:name=".Mapas.MapavendedorActivity" />
        <activity android:name=".Mapas.MapaActivity" />
        <activity
            android:name=".Detalles.DetallesZonaActivity"
            android:screenOrientation="locked" />
        <activity
            android:name=".Detalles.DetallesorganizacionActivity"
            android:screenOrientation="locked" />
        <activity
            android:name=".inicio"
            android:screenOrientation="locked"
            android:theme="@style/inicio">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Ventanas"
            android:label="@string/title_activity_ventanas"
            android:screenOrientation="locked"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="locked"
            android:theme="@style/primera" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <service
            android:name=".Eventuales.ServicioEventos"
            android:enabled="true"
            android:exported="true" />
    </application>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>